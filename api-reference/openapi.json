                    "data": {"type": "array", "items": {"type": "object"}},
                    "pagination": {"$ref": "#/components/schemas/PaginationInfo"}
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": ["API Keys"],
        "summary": "Create new API key",
        "operationId": "createApiKey",
        "security": [{"WorkspaceAuth": []}],
        "requestBody": {
          "required": true,
